{
  "name": "@datagd/web",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "build": "vite build",
    "build:stats": "node --no-warnings --experimental-specifier-resolution=node --loader ts-node/esm scripts/build-stats.ts"
  },
  "dependencies": {
    "ant-design-vue": "^3.2.15",
    "core-js": "^3.26.1",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@nabla/vite-plugin-eslint": "^1.4.1",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.28.0",
    "eslint-plugin-vue": "^9.8.0",
    "fs-extra": "^10.1.0",
    "sass": "^1.56.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4",
    "yaml": "^2.1.3"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "browser": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended",
      "@vue/typescript/recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020
    },
    "rules": {
      "@typescript-eslint/ban-ts-comment": "off",
      "@typescript-eslint/no-non-null-assertion": "off"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
